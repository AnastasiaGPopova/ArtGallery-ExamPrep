

        <main>
            <section id="profile-page">
                <h1>Full Profile Information</h1>
                <article class="profile-card">

                    <article class="profile-card-info">
                        <h2>Username: {{currentUser.username}}/h2>
                        <h3>Address: {{currentUser.address}}</h3>
                        <!--If the user has shared publications, separate their titles with a comma and a space (, )-->
                        <h4>Titles of shared posts by the user: 
                            {{#each shared}}</h4>
                            {{this}}
                            {{else}}<h4>
                            {{else}}Titles of shared posts by the user: Not yet.</h4>
                        {{/each}}

                        
                        <h4>Titles of which the user is the author: 
                            {{#each publicated}}
                            {{this}}
                        </h4>
                        {{else}}
                        <h4>Titles of which the user is the author: Not yet.</h4>

                    </article>

                    <article class="profile-card-icon">
                        <!-- Do not forget to change the path to the image -->
                        <img src="/img/vip-user.png" alt="art-image1">
                    </article>

                </article>
            </section>
        </main>
